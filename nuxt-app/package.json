    "vue": "3.4.25",
    "vue-router": "4.3.2"
  },
  "dependencies": {
    "@justeattakeaway/pie-assistive-text": "0.3.5",
    "@justeattakeaway/pie-button": "0.47.3",
    "@justeattakeaway/pie-card": "0.19.3",
    "@justeattakeaway/pie-chip": "0.6.1",
    "@justeattakeaway/pie-cookie-banner": "0.19.5",
